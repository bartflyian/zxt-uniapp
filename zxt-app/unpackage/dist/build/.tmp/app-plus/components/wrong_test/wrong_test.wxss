.wrong-test {
  box-shadow: 0 0 12px 0px rgba(128, 128, 128, 0.2);
  background: white;
  border-radius: 20rpx;
  font-family: PingFang SC;
  font-weight: bold;
  padding: 32rpx;
  font-size: 13px;
  margin-bottom: 33rpx;
  position: relative;
}
.wrong-test.choosing {
    -webkit-transform: translate(99rpx, 0);
            transform: translate(99rpx, 0);
}
.wrong-test .choose-area {
    position: absolute;
    left: -100rpx;
    top: 0;
    width: 40rpx;
    height: 100%;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
}
.wrong-test .choose-area .choose-btn {
      width: 40rpx;
      height: 40rpx;
      background: url(data:image/png;base64,) no-repeat;
      background-size: 40rpx 40rpx;
}
.wrong-test .choose-area .choose-btn.is-select {
        background: url(data:image/png;base64,) no-repeat;
        background-size: 40rpx 40rpx;
}
.wrong-test .time {
    display: block;
    width: 100%;
    font-size: 10px;
    color: #878c9e;
    line-height: 30rpx;
    margin-bottom: 61rpx;
}
.wrong-test .question {
    display: block;
    color: #323643;
    line-height: 36rpx;
    margin-bottom: 60rpx;
}
.wrong-test .options {
    margin-top: 20rpx;
}
.wrong-test .options .option {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
}
.wrong-test .options .option .option-num {
        -webkit-flex-basis: 72rpx;
                flex-basis: 72rpx;
        height: 72rpx;
        background: white;
        border: 2px solid #e5e9ef;
        border-radius: 50%;
        text-align: center;
        line-height: 68rpx;
        margin: 0 32rpx 40rpx 0;
}
.wrong-test .options .option .option-answer {
        -webkit-box-flex: 1;
        -webkit-flex: 1;
                flex: 1;
        line-height: 20px;
        margin-top: 6px;
}
.wrong-test .options .correct .option-num {
      background: #50C341;
      color: #fff;
      border: 0;
}
.wrong-test .options .wrong .option-num {
      background: #FF5A5A;
      color: #fff;
      border: 0;
}
.wrong-test .teacherword-anlysis .line-block {
    display: block;
    margin-bottom: 28rpx;
}
.wrong-test .teacherword-anlysis .line-block .correct {
      color: #50C341;
}
.wrong-test .teacherword-anlysis .line-block .wrong {
      color: #FF5A5A;
}
.wrong-test .teacherword-anlysis .line-block .answer-title {
      margin-right: 10rpx;
}
.wrong-test .teacherword-anlysis .line-block .title {
      margin-left: -16rpx;
      color: #FF7800;
      margin-right: 10rpx;
}
.wrong-test .teacherword-anlysis .anlysis {
    margin-top: 61rpx;
}
.wrong-test .teacherword-anlysis .anlysis .line-block {
      margin-bottom: 50rpx;
}

