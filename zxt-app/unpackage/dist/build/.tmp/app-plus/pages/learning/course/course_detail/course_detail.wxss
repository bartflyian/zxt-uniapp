.course-detail .course-ad {
  width: 100%;
}
.course-detail .course-ad ._img {
    width: 100%;
    height: 320rpx;
}
.course-detail .course-tab {
  border-top: 1px solid #F8F6FC;
  box-shadow: 0px 5px 8px 2px rgba(0, 0, 0, 0.1);
}
.course-detail .course-tab .tab-title {
    font-size: 13px;
    border-bottom: 2px solid #FF794A;
    margin: 0 32rpx;
    height: 25px;
    width: 40px;
    display: inline-block;
    text-align: center;
}
.course-detail .vedio-list {
  padding: 20rpx 40rpx;
}
.course-detail .vedio-list .list-li {
    overflow: hidden;
    height: 120rpx;
    line-height: 120rpx;
}
.course-detail .vedio-list .list-li .video-title {
      font-size: 13px;
}
.course-detail .vedio-list .list-li .video-title::before {
        margin-top: 40rpx;
        margin-right: 20rpx;
        content: '';
        width: 50rpx;
        height: 50rpx;
        float: left;
        background: url(data:image/png;base64,);
        background-size: 50rpx 50rpx;
}
.course-detail .booking-btn {
  width: 88rpx;
  height: 88rpx;
  border-radius: 50%;
  background: -webkit-linear-gradient(left, #f9d423 0%, #ff794a 100%);
  background: linear-gradient(90deg, #f9d423 0%, #ff794a 100%);
  position: fixed;
  bottom: 80rpx;
  right: 44rpx;
}
.course-detail .booking-btn::before {
    content: '';
    position: absolute;
    width: 50rpx;
    height: 50rpx;
    top: 18rpx;
    left: 20rpx;
    background: url(data:image/png;base64,);
    background-size: 50rpx 50rpx;
}
.course-detail .booking-btn::after {
    content: '\8BA2\9605';
    position: absolute;
    width: 50rpx;
    height: 50rpx;
    bottom: -60rpx;
    left: 20rpx;
    font-size: 12px;
    color: #ff794a;
}

